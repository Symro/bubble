                <!-- Main content -->
                <section class="content">

                    <!-- The time line -->
                    <ul class="timeline">
                                <!-- timeline time label -->
                                <li class="time-label">
                                    <span class="bg-blue">
                                        Activités des dernières 24h
                                    </span>
                                </li>
                                <!-- /.timeline-label -->
                            <% if(activity.length){ %>
                                <% _.each(activity, function(activity) { %>
                                <!-- timeline item -->
                                 <li>
                                    <% if(activity.action == "JOIN"){ %>
                                    <i class="fa fa-mail-forward bg-olive"></i>
                                    <% }else if(activity.action == "CREATE"){ %>
                                    <i class="fa fa-laptop bg-orange"></i>
                                    <% }else if(activity.action == "UPLOAD"){ %>
                                    <i class="fa fa-picture-o bg-maroon"></i>
                                    <% }else{  %>
                                    <i class="fa fa-user bg-aqua"></i>
                                    <% } %>

                                    <div class="timeline-item">
                                        <span class="time"><i class="fa fa-clock-o"></i> <%= moment(activity.createdAt).fromNow() %></span>
                                        <h3 class="timeline-header no-border"><a href="/admin/user/show/<%= activity.user.id %>"><%= activity.user.firstname %></a></h3> 
                                        <div class="timeline-body">
                                        <%= activity.action %> a <%= activity.type %> <% if(activity.info.length){ %> ( <%= activity.info %> ) <% } %>
                                            <small class="pull-right text-muted"> <%= activity.browser %> <%= activity.version %> / <%= activity.device %> <%= activity.os %> / <%= activity.ip %>  &nbsp;<i class="fa fa-arrow-circle-o-left"></i></small>
                                        </div>
                                    </div>
                                </li>
                                <!-- END timeline item -->
                                <% }) %>
                            <% } %>


